let arr = [10, 3, 15, 7, 9, 11, 4];
let i =0;
function isPrime(n){
  
if(n<=1){
   return false
}
  for(let k=2; k<= Math.sqrt(n); k++){
    if(n%k===0){
      return false
     
    }
  }
  return true
  
}

for(let j=0; j<arr.length;j++){
  if(isPrime(arr[j])){
    [arr[i],arr[j]]=[arr[j],arr[i]]
    i++;
  }
}
console.log(arr);
