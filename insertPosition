function insertPosition(arr, target) {
    let mp = new Map()
    
    for(let i = 0; i < arr.length; i++) {
      
        let rem = target- arr[i]
        if(mp.has(rem)){
            return [mp.get(rem),i]
        }else{
            mp.set(arr[i],i)
        }
    }
    return arr.length;
}
let arr = [1,3,5,6];
let target = 5;
console.log(insertPosition(arr, target));
